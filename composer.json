{
    "name": "bluebaytravel/link-to-inbox",
    "description": "Creates a link to the webmail inbox based on email address.",
    "license": "MIT",
    "authors": [{
        "name": "James Brooks",
        "email": "james@bluebaytravel.co.uk"
    }],
    "require": {
        "php": ">=5.5.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8|^5.0"
    },
    "autoload": {
        "psr-4": {
            "BlueBayTravel\\LinkToInbox\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BlueBayTravel\\Tests\\LinkToInbox\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
